<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#e2e5d3"
    android:layout_width="match_parent"
    tools:context=".UserMainScreen"
    android:layout_height="match_parent">


    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:id="@+id/appBar"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/colorWhite" />

    </com.google.android.material.appbar.AppBarLayout>


    <RelativeLayout
        android:layout_below="@+id/appBar"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="#fff"
        android:id="@+id/rsd0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textview1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/amiko_bold"
            android:text="What would "
            android:textColor="#000"
            android:textSize="28sp" />

        <TextView
            android:id="@+id/textview2"
            android:layout_marginLeft="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:textSize="27sp"
            android:fontFamily="@font/amiko_bold"
            android:textColor="#000"
            android:layout_below="@+id/textview1"
            android:text="you like to eat today?"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/igii"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_below="@id/rsd0"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rl1"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/lnr3"
                android:layout_width="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/chicImg"
                    android:layout_centerHorizontal="true"
                    android:layout_width="60dp"
                    android:foregroundGravity="center"
                    android:layout_height="60dp"
                    android:src="@drawable/chickenrice"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ayam Gepok"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textSize="14sp"
                    android:textColor="#000"
                    android:fontFamily="@font/basic"
                    android:layout_below="@id/chicImg"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/lnr4"
                android:layout_toRightOf="@id/lnr3"
                android:layout_width="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="10dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/rice"
                    android:layout_centerHorizontal="true"
                    android:layout_width="60dp"
                    android:foregroundGravity="center"
                    android:layout_height="60dp"
                    android:src="@drawable/rice"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Asian Cuisine"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textSize="15sp"
                    android:textColor="#000"
                    android:fontFamily="@font/basic"
                    android:layout_below="@id/rice"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/lnr6"
                android:layout_toRightOf="@id/lnr5"
                android:layout_width="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="20dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/spag"
                    android:layout_centerHorizontal="true"
                    android:layout_width="60dp"
                    android:foregroundGravity="center"
                    android:layout_height="60dp"
                    android:src="@drawable/pasta"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Spaghetti"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textSize="15sp"
                    android:textColor="#000"
                    android:fontFamily="@font/basic"
                    android:layout_below="@id/spag"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/lnr5"
                android:layout_toRightOf="@id/lnr4"
                android:layout_width="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="15dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/crab"
                    android:layout_centerHorizontal="true"
                    android:layout_width="60dp"
                    android:foregroundGravity="center"
                    android:layout_height="60dp"
                    android:src="@drawable/crab"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Shellout"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textSize="15sp"
                    android:textColor="#000"
                    android:fontFamily="@font/basic"
                    android:layout_below="@id/crab"/>

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="10dp"
            android:id="@+id/rl2"
            android:layout_below="@id/rl1"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/lnr1"
                android:layout_width="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="15dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/mojitoImg"
                    android:layout_centerHorizontal="true"
                    android:layout_width="60dp"
                    android:foregroundGravity="center"
                    android:layout_height="60dp"
                    android:src="@drawable/mojito"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mojitos"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textSize="17sp"
                    android:textColor="#000"
                    android:fontFamily="@font/basic"
                    android:layout_below="@id/mojitoImg"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_marginBottom="10dp"
                android:id="@+id/lnr2"
                android:layout_toRightOf="@id/lnr1"
                android:layout_width="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="15dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/hotbrewImg"
                    android:layout_centerHorizontal="true"
                    android:layout_width="60dp"
                    android:foregroundGravity="center"
                    android:layout_height="60dp"
                    android:src="@drawable/food"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Hot Brew"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textSize="17sp"
                    android:textColor="#000"
                    android:fontFamily="@font/basic"
                    android:layout_below="@id/hotbrewImg"/>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

    <WebView
        android:id="@+id/wbe"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@+id/igii"/>

</RelativeLayout>